@model IEnumerable<CRUD_application_2.Models.User>

<h2>Search Results</h2>

<table>
    <tr>
        <th>Name</th>
        <th>Email</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>*@
        </tr>
    }
</table>
